{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "requireConfigFile": false
    },
    "parser": "@babel/eslint-parser",
    "rules": {
        "no-console": 0, 
        "no-empty": 0,
        "no-irregular-whitespace":0,
        "max-len": [1, 900, 2, {"ignoreComments": true}]
    }
}
